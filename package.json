{
	"name": "my-app",
	"version": "0.2.0",
	"author": "Your Name",
	"description": "React-TypeScript-Electron sample with Create React App and Electron Builder",
	"homepage": ".",
	"main": "build/electron/main.js",
	"private": true,
	"dependencies": {
		"@element-ts/chlorine": "latest",
		"@material-ui/core": "latest",
		"@material-ui/icons": "latest",
		"@testing-library/jest-dom": "^5.11.4",
		"@testing-library/react": "^11.1.0",
		"@testing-library/user-event": "^12.1.10",
		"@types/codemirror": "latest",
		"@types/electron-devtools-installer": "^2.2.0",
		"@types/jest": "^26.0.15",
		"@types/node": "^12.0.0",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"blockly": "latest",
		"codemirror": "latest",
		"electron-devtools-installer": "^3.2.0",
		"electron-is-dev": "^2.0.0",
		"electron-prompt": "^1.7.0",
		"electron-reload": "^1.5.0",
		"react": "^17.0.2",
		"react-blockly": "latest",
		"react-codemirror2": "latest",
		"react-dom": "^17.0.2",
		"react-scripts": "4.0.3",
		"typescript": "^4.1.2",
		"web-vitals": "^1.0.1",
    "smalltalk": "latest"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"postinstall": "electron-builder install-app-deps",
		"electron:dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
		"electron:build": "yarn build && tsc -p electron && cp electron/alloy/blockloy-alloy-integration.jar build/electron/alloy/blockloy-alloy-integration.jar && electron-builder -w",
		"eject": "react-scripts eject"
	},
	"build": {
		"appId": "com.example.shakudo",
		"extends": null,
		"files": [
			"build/**/*"
		],
		"directories": {
			"buildResources": "assets"
		},
		"extraResources": [
			{
				"from": "build/electron/alloy/blockloy-alloy-integration.jar",
				"to": "blockloy-alloy-integration.jar"
			}
	  ]
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"concurrently": "^6.0.0",
		"cross-env": "^7.0.3",
		"electron": "^12.0.1",
		"electron-builder": "^22.10.5",
		"wait-on": "^5.3.0"
	}
}
